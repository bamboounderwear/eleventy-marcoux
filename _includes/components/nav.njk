<nav>
  {% set navPages = collections.all | eleventyNavigation %}
  <button id="hamburger-btn">&#9776;</button>
  <div id="menu-container" class="menu-container">
    <button id="close-btn" class="close-btn">&times;</button> <!-- This is a Unicode character for a multiplication sign, often used as a close icon -->
    <ul>
      {%- for entry in navPages %}
        <li>
          <a href="{{ entry.url | url }}"{% if entry.url == page.url %} data-current="current item"{% endif %}>{{ entry.title }}</a>
        </li>
      {%- endfor %}
    </ul>
  </div>
</nav>